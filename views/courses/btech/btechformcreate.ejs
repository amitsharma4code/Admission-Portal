<%-include('../header.ejs')%>

    <section class="min-vw-100">
        <section id="registration">
            <div class="container">
                <h1 class="text-center pt-5">B.Tech Registration Form</h1>
                <h1 class="text-center text-uppercase pb-2" style="color:
                    #ed5a25">
                    Do Register!
                </h1>
                <p class="text-center" style="color: #fff; background-color:
                    #35b9dd">
                    In Case any query, Kindly Contact: 7974978812
                </p>
                <div class="row">
                    <div class="col-lg-6">
                        <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
                            <ol class="carousel-indicators">
                                <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
                                <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
                                <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
                            </ol>
                            <div class="carousel-inner">
                                <div class="carousel-item active">
                                    <img class="d-block w-100" src="c1.png" alt="First slide" style="
                                        background-position: center;
                                        width: 100%;
                                        height: 700px;
                                        " />
                                </div>
                                <div class="carousel-item">
                                    <img class="d-block w-100" src="c2.jpg" alt="Second slide" style="
                                        background-position: center;
                                        width: 100%;
                                        height: 700px;
                                        " />
                                </div>
                                <div class="carousel-item">
                                    <img class="d-block w-100" src="c3.webp" alt="Third slide" style="
                                        background-position: center;
                                        width: 100%;
                                        height: 700px;
                                        " />
                                </div>
                            </div>
                            <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button"
                                data-slide="prev">
                                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                                <span class="sr-only">Previous</span>
                            </a>
                            <a class="carousel-control-next" href="#carouselExampleIndicators" role="button"
                                data-slide="next">
                                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                                <span class="sr-only">Next</span>
                            </a>
                        </div>
                    </div>
                    <!-- Slider Div end -->
                    <div class="col-lg-6">
                        <form action="/btechforminsert" method="post" enctype="multipart/form-data">
                            <!-- <input type="text" name="user_id" value="63fef8ace882c45419026c2d"> -->
                            <div class="mb-3 mt-1">
                                <label class="mb-2">Username:</label>
                                <input type="text" name="name" value="<%=n%>" class="form-control" style="border: 1px solid #35b9dd" />
                            </div>
                            <div class="mb-3">
                                <label class="mb-2">Email:</label>
                                <input type="email" name="email" value="<%=email%>" class="form-control"
                                     style="border-radius: 25px; border: 1px solid #35b9dd" />
                            </div>
                            <div class="mb-3">
                                <label class="mb-2">Mobile No.</label>
                                <input type="number" name="number" class="form-control" placeholder="Enter Mobile Number" style="border-radius: 25px; border: 1px
                                    solid #35b9dd" />
                            </div>
                            <div class="mb-3">
                                <label class="mb-2">D.O.B:</label>
                                <input type="date" name="dob" class="form-control" style="border-radius: 25px; border: 1px
                                    solid #35b9dd" />
                            </div>
                            <div class="mb-3">
                                <label class="mb-2">Gender</label>
                                <br />
                                <input type="radio" name="gender" value="male" />&nbsp;Male
                                <input type="radio" name="gender" value="female" />&nbsp;Female
                                <br />
                            </div>
                            <div class="mb-3">
                                <label>Address:</label>
                                <textarea class="form-control" name="address" style="border-radius: 25px; border: 1px
                                    solid #35b9dd"></textarea>
                            </div>
                            <div class="mb-3">
                                <label>Select College</label>
                                <select name="college" class="form-control" style="border-radius: 25px; border: 1px
                                    solid #35b9dd">
                                    <option>Select</option>
                                    <option>MITS</option>
                                    <option>RJIT</option>
                                    <option>ITM</option>
                                    <option>NITM</option>
                                </select>
                            </div>
                            <div class="mb-3">
                                <label>Course</label>
                                <select name="course" class="form-control" style="border-radius: 25px; border: 1px
                                    solid #35b9dd">
                                    <option>Select</option>
                                    <option>B.Tech</option>
                                </select>
                            </div>
                            <div class="mb-3">
                                <label>Branch</label>
                                <select name="branch" class="form-control" style="border-radius: 25px; border: 1px
                                    solid #35b9dd">
                                    <option>Select</option>
                                    <option>CSE</option>
                                    <option>IT</option>
                                    <option>EC</option>
                                    <option>ET</option>
                                    <option>ME</option>
                                    <option>EE</option>
                                </select>
                            </div>
                            <div class="text-center">
                                <button class="btn my-4" style="background-color: #35b9dd; color:
                                    #fff" type="submit">
                                    Submit
                                </button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </section>
    </section>

    <%-include('../footer.ejs')%>